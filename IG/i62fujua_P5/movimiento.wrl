DEF Objeto Transform{
    translation 100 50 50
    
    children[
        DEF TSensor1 TouchSensor{},
        Shape {
            
            appearance Appearance {
                material Material { }
            }

            geometry Box {
            size 20 20 20
            }
        }
    ]
}

#Sensor de tiempo
#	Duraci贸n: 4 segundos
DEF Time1 TimeSensor{
	cycleInterval 3.0
	startTime 0.0
	stopTime 0.0
	loop FALSE
}

#Interpoladores de posici贸n
#Cuerpo
DEF MovCuerpo OrientationInterpolator{
	key[0.0, 1.0]
	keyValue[
        0 0 0  0,
		1 0 0  0.6
    ]
}

#CuerpoUp
DEF MovCuerpoUp OrientationInterpolator{
    key[0.0, 1.0]
    keyValue[
        0 0 0  0,
        0 1 0  -0.3
    ]
}

#CuerpoDown
DEF MovCuerpoDown OrientationInterpolator{
    key[0.0, 1.0]
    keyValue[
        0 0 0  0,
        1 0 0  0.6
    ]
}

